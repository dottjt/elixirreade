<h2>Show Project</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @project.name %>
  </li>

  <li>
    <strong>Display name:</strong>
    <%= @project.display_name %>
  </li>

  <li>
    <strong>Excerpt:</strong>
    <%= @project.excerpt %>
  </li>

  <li>
    <strong>Featured image:</strong>
    <%= @project.featured_image %>
  </li>

</ul>

<span><%= link "Edit", to: project_path(@conn, :edit, @project) %></span>
<span><%= link "Back", to: project_path(@conn, :index) %></span>
